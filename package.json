{
  "name": "apptension-tools",
  "version": "4.5.4",
  "description": "Common tools used in Apptension's projects",
  "repository": "https://github.com/apptension/apptension-tools.git",
  "main": "index.js",
  "keywords": [
    "apptension",
    "gulp",
    "apptension-tools"
  ],
  "scripts": {
    "test": "mocha --require ./test/enable-power-assert",
    "test:watch": "mocha --require ./test/enable-power-assert --watch",
    "release": "npm run release-patch",
    "release-patch": "git commit -a -m 'Build dist/'; npm version patch && git push origin master --tags && npm publish",
    "release-minor": "git commit -a -m 'Build dist/'; npm version minor && git push origin master --tags && npm publish",
    "release-major": "git commit -a -m 'Build dist/'; npm version major && git push origin master --tags && npm publish"
  },
  "author": {
    "name": "Patryk Ziemkowski",
    "email": "pziemkowski@apptension.com"
  },
  "license": "ISC",
